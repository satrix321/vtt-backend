{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*.ts",
  ]
}
  